#xfce-mcs-manager &

# STARTUP --------------------------------------------------------

compton &
sleep 1s

wallpaper='w.jpg'

feh --bg-scale /home/dava/.3installed/wallpaper/$wallpaper &

/home/dava/.1homepc-config/home/setlayout 0 3 2 0 &

volumeicon &
sleep 1s

numlockx &
sleep 1s

opensnap -d &
sleep 1s

xset -dpms; xset s off; xset -b &

tint2 &
sleep 1s

conky &
sleep 1s

guake &
sleep 1s

gmrun &
sleep 1s

sleep 1s
killall gmrun

sleep 1s
deluge &

sleep 1s
xscreensaver -no-splash &



















