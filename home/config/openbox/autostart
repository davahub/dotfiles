# STARTUP --------------------------------------------------------

wallpaper='w.jpg'

homeConfig="/home/dava/.1homepc-config/home/config"

compton &

sleep 1s
guake &

feh --bg-scale /home/dava/.3installed/wallpaper/$wallpaper &

/home/dava/.1homepc-config/home/setlayout 0 3 2 0 &

volumeicon &

google-chrome-stable &

numlockx &

opensnap -d &

xset -dpms; xset s off; xset -b &

tint2 &

conky &

gmrun &

sleep 1s
killall gmrun

deluge &
















