  "header 1": {
    "prefix": "mdh1",
    "body": ["# ${0} #"],
    "description": "Add header level 1"
  },
  "header 2": {
    "prefix": "mdh2",
    "body": ["## ${0} ##"],
    "description": "Add header level 2"
  },
  "header 3": {
    "prefix": "mdh3",
    "body": ["### ${0} ###"],
    "description": "Add header level 3"
  },
  "header 4": {
    "prefix": "mdh4",
    "body": ["#### ${0} ####"],
    "description": "Add header level 4"
  },
  "header 5": {
    "prefix": "mdh5",
    "body": ["##### ${0} #####"],
    "description": "Add header level 5"
  },
  "header 6": {
    "prefix": "mdh6",
    "body": ["###### ${0} ######"],
    "description": "Add header level 6"
  },
  "comment insert": {
    "prefix": "mwc",
    "body": ["# ${0}"],
    "description": ""
  },  
  "Link": {
    "prefix": ["mdlink", "mdl"],
    "body": [
      "[${1}](${2})"
    ],
    "description": "insert link"
  },
  "Shell for mediawiki": {
    "prefix": "mdmediash",
    "body": [
      "<syntaxhighlight lang='shell' line='line'>",
      "${0}",
      "</syntaxhighlight>"
    ],
    "description": "shell insert"
  },
  "Shell for markdown": {
    "prefix": "mdsh",
    "body": [
      "```sh",
      "${0}",
      "```"
    ],
    "description": "shell insert"
  },
  "Java": {
    "prefix": "mdjava",
    "body": [
      "<syntaxhighlight lang='java' line='line'>",
      "${0}",
      "</syntaxhighlight>"
    ],
    "description": "shell insert"
  },
  "Bold": {
    "prefix": "mdb",
    "body": [
      "**${0}**  "
    ],
    "description": "bold text"
  }
}

